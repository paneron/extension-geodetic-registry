{
  "name": "@riboseinc/paneron-extension-geodetic-registry",
  "version": "1.0.0",
  "description": "Geodetic Registry structured data plugin",
  "main": "plugin.js",
  "repository": "https://github.com/riboseinc/plugin-geodetic-registry",
  "author": {
    "name": "Ribose Inc.",
    "email": "open.source@ribose.com"
  },
  "scripts": {
    "build": "rm -r compiled dist; mkdir compiled dist; tsc --outDir compiled; babel compiled --out-dir dist; cp package.json dist/; rsync -a --include '*/' --include '*.d.ts' --include '*.js.map' --exclude '*' compiled/ dist/"
  },
  "paneronExtension": {
    "title": "ISO Geodetic Registry",
    "iconURL": "https://isotc211.geolexica.org/assets/logo-iso-noninverted.svg",
    "featured": true,
    "requiredHostAppVersion": "^1.0.0-beta1"
  },
  "files": [
    "README.adoc",
    "*.js",
    "*.js.map",
    "*.d.ts",
    "classes/*.js",
    "classes/*.js.map",
    "classes/*.d.ts"
  ],
  "resolutions": {
    "@types/react": "17.0.53"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@blueprintjs/core": "~4.17.6",
    "@blueprintjs/datetime": "~4.4.25",
    "@blueprintjs/icons": "~4.14.3",
    "@blueprintjs/select": "~4.9.10",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@riboseinc/paneron-extension-kit": "2.0.0",
    "@riboseinc/paneron-registry-kit": "2.0.0",
    "@types/node": "^16.11.7",
    "@types/react": "17.0.53",
    "electron": "^15.5.7",
    "electron-log": "^4.3.0",
    "immutability-helper": "^3.1.1",
    "react": "^17.0.2",
    "typescript": "~4.6.0"
  },
  "license": "MIT"
}
